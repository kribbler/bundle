ul.products{
	width: auto;
}
.listing_products{
	width: 860px;
}
.product_item{
	width: 44%;
	min-height: 400px;
}

.product_item h2{
	font-size: 23px;
    font-weight: normal;
    margin-bottom: 1em;
}
.product_item h2 a{
	color: #000;
}

.my_product_description{
	float: left;
	width: 45%;
}

.image_container{
	float: right;
}

.my_product_attribute_blue{
	padding: 10px 15px;
	background-color: #1f6086;
	color: #fff;
	font-weight: bold;
	font-size: 16px;
	margin-right: 10px;
}

#home_sheader{
	height: 620px;
	display: block;
	
}
.home_top{
	display: block;
	float: left;
	width: 469px;
	padding: 30px;
	height: 281px;
	margin-top: 20px;
	-webkit-border-radius: 20px;
	-moz-border-radius: 20px;
	border-radius: 20px;
	
	background: url(images/header_tyre_search.png) top right no-repeat rgb(0, 0, 0) transparent;   /* Fallback for web browsers that doesn't support RGBa */
	background: url(images/header_tyre_search.png) top right no-repeat rgba(0, 0, 0, 0.9);   /* RGBa with 0.6 opacity */
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#99000000, endColorstr=#99000000);  /* For IE 5.5 - 7*/
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#99000000, endColorstr=#99000000)";  /* For IE 8*/
}
#home_search_box{
	
}

#home_indep{
	width: 468px;
	background-image: none;
	margin-left: 30px;
}
#home_indep h1{
	color: #e4d100;
	font-size: 22px;
	line-height: 30px;
	font-weight: normal;
}
#home_indep p {
	color: #fff;
	margin-top: 15px;
}
#home_indep p img{
	float: right;
	margin-top: -71px;
	margin-right: 30px;
}

.header_slogan{
	width: 80%; margin: 0 auto;
	-webkit-border-radius: 20px;
	-moz-border-radius: 20px;
	border-radius: 20px;
	margin-top: 50px;
	background: top right no-repeat rgb(0, 0, 0) transparent;   /* Fallback for web browsers that doesn't support RGBa */
	background: top right no-repeat rgba(0, 0, 0, 0.6);   /* RGBa with 0.6 opacity */
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#99000000, endColorstr=#99000000);  /* For IE 5.5 - 7*/
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#99000000, endColorstr=#99000000)";  /* For IE 8*/
	color: #fff;
	text-align: center;
	font-size: 24px;
	padding: 20px;
}
#home_search_box h1{
	color: #fff;
	font-size: 20px;
	text-transform: uppercase;
	border-bottom: 0;	
}

#home_search_box p{
	margin: 10px 0;
	color: #fff;
	font-weight: bold;
}

#enter_your_reg{
	width: 220px;
	height: 47px;
	display: block;
	cursor: text;
	background: url(images/reg_checker_input.png) no-repeat;
	border: medium none;
    color: #000 !important;
    display: block;
    font-family: 'uknumberplateregular',impact,sans-serif;
    font-size: 26px;
    font-weight: bold;
    height: 47px;
    letter-spacing: -2px;
    line-height: 47px;
    outline: medium none;
    padding: 0 10px;
    text-align: center;
    width: 200px;
    text-transform: uppercase;
}
.header_right{
	float: left;
	display: block;
	width: 529px;
}

.content_about{
	width: 1100px !important;
	clear: both;
	margin-top: 30px;
	margin-bottom: 30px;
}
.content_about .grid_12{
	width: 1100px !important;
}
.content_about h1, .entry-content h1{
	font-size: 35px;
	font-weight: normal;
	margin-bottom: 30px;
}
.content_about img, .content_block img{
	float: right;
	padding: 0 50px 0 150px;
}
.content_about p, .content_block p{
	font-size: 14px;
	line-height: 20px;
}

.content_about .content_grid_4, .content_block .content_grid_4{
	width: 280px !important;
	padding-left: 60px;
	background: url(images/quotes.png) top left no-repeat;
}

.content_about .content_grid_2, .content_block .content_grid_2{
	margin-right: 0 !important;
}

.entry-header{
	display: none;
}
.super_search{
	color: #fff;
	width: 100% !important;
}
.supersearch_submit{
	margin:7px;
}
.super_search1{
	width: 21%;
	float: left;
	padding-right: 20px;
}
.super_search1 select, .super_search2 select{
	width: 100% !important;
}

.super_search2{
	width: 40%;
	float: left;
}

.super_search2 #searchsubmit{
	background: none !important;
	background: url(images/search_tyres.png) center no-repeat !important;
	width: 189px !important;
	height: 33px !important;
	display: block !important;
	position: relative !important;
	background-position: 0 0;
}

.page-title{
	text-transform: none !important;
}
.important_note h2{
	font-size: 24px;
	font-weight: normal;
	margin: 20px 0 10px;
}
.important_note p{
	font-size: 14px;
}

#search_tyre_top1{
	width: 40%; float: left;
}
#search_tyre_top2{
	width: 40%; float: left;
	padding-left: 40px;
	display: none;
}

#vrm{
	margin: 10px 0;
	color: #666;
}
#b_invoke{
	margin-left: 15px;
}

.span_blue_bg, .span_no_bg{
	display: inline-block;
	background-color: #1f6086;
	padding: 10px 20px;
	color: #fff;
	font-weight: bold;
	font-size: 30px;
	margin-right: 10px;
}

.span_no_bg{
	background: none;
	color: #000;
	padding: 10px;
}

#tyre_service_no_results, #ajax_loading{
	clear: both;
	padding-top: 10px;
	display: block;
	color: yellow;
}

.content_wrapper{
	width: 100%;
}

.content_block{
	clear: both;
	margin-top: 40px;
}

.doubleSlider-2 {
    height: 400px;
    overflow: hidden;
    width: 510px;
}

.doubleSlider-2 .slider{
	height: 100%;
}

.doubleSlider-2 .slider .item {
	position: relative;
	top: 0;
	left: 0;
	width: 510px;
	margin: 0;
}

.doubleSlider-2 .slider .item img {
	/*width:100% !important;*/
	width:inherit !important;
	max-width:510px !important;
	height:auto !important;
	margin:0 auto;
}

.gbtr_product_slider_wrapper, .gbtr_items_slider_wrapper{
	max-width: 1100px;
}

.gbtr_items_slider li{
	width: 30%;
}

.gbtr_items_slider .my_product_description{
	width: 60%;
}
.gbtr_items_slider .image_container{
	width: 30%;
}

.gbtr_light_footer_wrapper, .gbtr_light_footer_no_widgets{
	display: none;
}
.mybutton_product_type_variable {
	background-color: #dcca00 !important;
	-webkit-border-radius: 5px !important;
	-moz-border-radius: 5px !important;
	border-radius: 5px !important;
    color: #000 !important;
    display: inline-block;
    float: right;
    font-size: 17px;
    font-weight: bold;
    padding: 10px 30px !important;
    text-transform: uppercase;
}

.product_item .price{
	font-weight: bold;
	font-size: 24px;
	position: relative;
    top: 12px;
}

.gbtr_footer_widget_credit_cards{
	width: auto;
	float: right;
}
