	border-top: none !important;
}

.bottom_wrapper a{
	color: #fff;
}
.gbtr_footer_widget_copyrights{
	float: left;
	text-align: left;
	color: #fff !important;
	text-transform: none !important;
}

.gbtr_footer_widget_credit_cards{
	text-align: right;
	color: #fff;
}

.gbtr_dark_footer_wrapper{
	background-color: #040708 !important;
}

.gbtr_dark_footer_wrapper .grid_3{
	width: 100% !important;
}

.gbtr_light_footer_no_widgets{
	background-color: #1a3f56 !important;
}

.gbtr_tools_wrapper{
	color: #fff !important;
	/*background-color:rgba(26,63,86,0.9) !important;*/
	background: none !important;
	height: 78px !important;
}
#h_call{
	font-size: 30px;
    padding-left: 30px;
}

.gbtr_tools_info{
	color: #fff !important;
    font-size: 25px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    text-transform: none;
}

#global_wrapper{
	background: url(images/bg_page.png) top center no-repeat;
}
.home_class{
	background: url(images/bg_sheader.png) top center no-repeat !important;
}

.gbtr_header_wrapper{
	background-color:rgba(255,255,255,0.95) !important;
