@import url("../theretailer/style.css");

@font-face{
	font-family:'uknumberplateregular';
	src:url('js/uknumberplate.eot');
	src:url('js/uknumberplate.eot?#iefix') format('embedded-opentype'),
	url('js/uknumberplate.woff') format('woff'),
	url('js/uknumberplate.ttf') format('truetype'),
	url('js/uknumberplate.svg#uknumberplateregular') format('svg');font-weight:normal;font-style:normal
}
