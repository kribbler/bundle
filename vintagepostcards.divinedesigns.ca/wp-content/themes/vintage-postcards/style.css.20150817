body {
    background-image: url('images/paper.png');
    font-family: 'Titillium Web', sans-serif;
    clear: both;
    background-repeat: repeat-x repeat-y;
}

.site-inner {
    min-width: auto;
    width: auto;
    max-width: auto !important;
    clear: both;
}

.nav-primary, .nav-primary > .wrap,.nav-primary > .wrap >  ul {
    display: block;
    width: 100%;
    height: auto;
    margin: 0;
    padding: 0;
    border: 0;
    clear: both;
}

.wcc_block > ul {
	padding-top: 2.5em;
	padding-bottom: 2.5em;
	background-image: url('images/trim.png'),url('images/trim.png');
	background-position: left top,left bottom,top left;
	background-repeat: repeat-x,repeat-x;
	background-color: #1E2222;
}

.nav-primary > .wrap {
    background-image: url('images/menu-dash.png'),url('images/menu-dash.png'),linear-gradient(to right,#317D87,#519199);
    background-position: top 0.1250em left,bottom 0.1250em left,top left;
    background-repeat: repeat-x,repeat-x,no-repeat;
    background-size: 2.5em 0.1250em,2.5em 0.1250em,auto 100%;
    width: 100%;
    max-width: 100%;
    max-height: 100%;
    padding-left: 0;
    padding-right: 0;
    border-left: 0;
    border-right: 0;
    margin-left: 0;
    margin-right: 0;
    background-color: #317D87 !important;
}

.nav-primary > .wrap >  ul > li {
    display: inline-block;
    list-style: none;
    width: auto;
    height: auto;
    padding: 0;
    border: 0;
    margin-top: 0.25em;
    margin-bottom: 0.25em;
    background-image: url('images/menu-staple.png'),linear-gradient(to right,#317D87,#519199);
    background-position: top right,top left;
    background-repeat: no-repeat, no-repeat;
    background-size: auto 100%,100%;
}

.nav-primary > .wrap >  ul > li:hover > a {
    color: #000;
    text-decoration: none !important;
}

.nav-primary > .wrap > ul {
    display: inline-block;
    overflow: visible;
    /* min-height: 1.0em; */
    width: auto;
    font-size: 14pt;
    padding: 0.125em 0;
    clear: both;
}

.nav-primary > .wrap > ul > li.current-menu-item {
		position: relative;
    overflow: visible;
    background-image: url('images/menu-clip.png'),url('images/menu-paper.png');
    background-position: top -0.50em right 0.50em,top right;
    background-repeat: no-repeat,no-repeat;
    background-size: 1.0em,100% 5em;
    clear: both;
    background-origin: content-box;
    margin-top: -0.5em;
    margin-bottom: 0.5em;
    color: #317D87 !important;
    padding-bottom: 0.5em;
}

.nav-primary > .wrap > ul > li:hover {
    position: relative;
    overflow: visible;
    background-image: url('images/menu-clip.png'),url('images/menu-paper.png');
    background-position: top -0.5em right 0.5em,top right;
    background-repeat: no-repeat,no-repeat;
    background-size: 1.0em,100% 5em;
    clear: both;
    background-origin: content-box;
    color: #317D87 !important;
}

.genesis-nav-menu > ul > li > a {
    font-family: Gill Sans,Gill Sans MT,Calibri,sans-serif; 
    font-size: 14pt; 
    font-style: normal; 
    font-variant: normal; 
    color: #FFFFFF;
    font-weight: bold;
    margin: 0;
    padding: 1.0em 1.0em 0em 1.0em;
    border: 0;
    text-transform: uppercase;
    text-decoration: none;
    text-align: center;
    clear: both;
    vertical-align: middle;
}

.genesis-nav-menu > ul > li > a:hover {
    color: #F0F1FE;
    clear: both;
}

.genesis-nav-menu > ul > li.current-menu-item> a {
    color: #FFF;
    clear: both;
}

.genesis-nav-menu > ul > li.current-menu-item > a:hover {
    color: #F0F1FE;
    clear: both;
}

